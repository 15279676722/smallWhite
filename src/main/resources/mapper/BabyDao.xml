<?xml version="1.0" encoding="UTF-8"?><!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"><mapper namespace="com.example.smallwhite.mybatis.dao.BabyDao">    <cache/>    <resultMap type="com.example.smallwhite.mybatis.entity.Baby" id="BabyMap">        <result property="id" column="id" jdbcType="VARCHAR"/>        <result property="name" column="name" jdbcType="VARCHAR"/>        <collection property="image" ofType="java.util.List" column="parent_id=id" select="com.example.smallwhite.mybatis.dao.BabyimageDao.queryByParentId"/>    </resultMap>    <!--查询单个-->    <select id="queryById" resultMap="BabyMap">        select id,               name        from baby<!--            <bind name="likeId" value="'%'+id+'%'"/>-->        where id = #{id}    </select>    <!--查询指定行数据-->    <select id="queryAllByLimit" resultMap="BabyMap">        select id,               name        from baby        limit #{offset}, #{limit}    </select>    <!--通过实体作为筛选条件查询-->    <select id="queryAll" resultMap="BabyMap">        select        id, name        from baby        <where>            <if test="id != null and id != ''">                and id = #{id}            </if>            <if test="name != null and name != ''">                and name = #{name}            </if>        </where>    </select>    <!--新增所有列-->    <insert id="insert" >        insert into baby(id,name)        values (#{id},#{name})    </insert>    <insert id="insertBatch" keyProperty="id" useGeneratedKeys="true">        insert into baby(name)        values        <foreach collection="entities" item="entity" separator=",">            (#{entity.name})        </foreach>    </insert>    <insert id="insertOrUpdateBatch" keyProperty="id" useGeneratedKeys="true">        insert into baby(name)        values        <foreach collection="entities" item="entity" separator=",">            (#{entity.name})        </foreach>        on duplicate key update        name = values(name)    </insert>    <!--通过主键修改数据-->    <update id="update">        update baby        <set>            <if test="name != null and name != ''">                name = #{name},            </if>        </set>        where id = #{id}    </update>    <!--通过主键删除-->    <delete id="deleteById">        delete        from baby        where id = #{id}    </delete>    <insert id="insertBatchRetIds" useGeneratedKeys="true" keyProperty="id" parameterType="java.util.List">        insert baby (id,name) values            <foreach collection="list" item="item" separator="," index="index">                (#{item.id},#{item.name})            </foreach>    </insert></mapper>